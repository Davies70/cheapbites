datasource db {
  provider = "sqlite"  // Using SQLite
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model PlaceCache {
  key   String   @id
  value String
  ttl   DateTime  
}

model PlacesCache {
  key   String   @id
  value String
  ttl   DateTime  
}

model ImageCache {
  key   String   @id
  value String
  ttl   DateTime 
}

model ImagesCache {
  key   String   @id
  value String
  ttl   DateTime 
}

model OtherCache {
  key   String   @id
  value String
  ttl   DateTime 
}

